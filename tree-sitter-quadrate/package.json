{
  "name": "tree-sitter-quadrate",
  "version": "0.1.0",
  "description": "Tree-sitter grammar for Quadrate programming language",
  "main": "bindings/node",
  "keywords": [
    "tree-sitter",
    "parser",
    "quadrate",
    "syntax"
  ],
  "author": "Quadrate Project",
  "license": "MIT",
  "dependencies": {
    "nan": "^2.17.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.24.0"
  },
  "tree-sitter": [
    {
      "scope": "source.quadrate",
      "file-types": [
        "qd"
      ],
      "highlights": "queries/highlights.scm",
      "indents": "queries/indents.scm"
    }
  ]
}
